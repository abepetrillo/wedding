{
  "root": "dist/",
  "https_only": true,
  "max_body_size": "10M",
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/server/": {
      "origin": "https://${API_HOST}/"
    }
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "/**.js": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    },
    "**.css": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    },
    "/**.ttf": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    },
    "/**.eot": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    },
    "/**.woff": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    },
    "/**.woff2": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    }
  }
}
